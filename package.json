{
  "name": "@agney/react-avatar",
  "version": "1.0.2",
  "license": "MIT",
  "private": false,
  "description": "Create Image Avatars with text based fallbacks.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "react",
    "avatar",
    "text avatar",
    "image"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "release": "np",
    "postrelease": "yarn run build-storybook && gh-pages -d storybook-static"
  },
  "peerDependencies": {
    "react": ">=16.8",
    "styled-components": ">=4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint --fix"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "Agney Menon <agney@outlook.in>",
  "module": "dist/react-avatar.esm.js",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@storybook/addon-actions": "^5.3.14",
    "@storybook/addon-docs": "^5.3.14",
    "@storybook/addon-links": "^5.3.14",
    "@storybook/addons": "^5.3.14",
    "@storybook/react": "^5.3.14",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^10.0.1",
    "@types/jest": "^25.1.3",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/styled-components": "^5.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-polished": "^1.1.0",
    "gh-pages": "^2.2.0",
    "husky": "^4.2.3",
    "np": "*",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-is": "^16.13.0",
    "styled-components": "^5.0.1",
    "ts-loader": "^6.2.1",
    "tsdx": "^0.12.3",
    "tslib": "^1.11.0",
    "typescript": "^3.8.2"
  },
  "dependencies": {
    "polished": "^3.4.4"
  },
  "jest": {
    "globals": {
      "__DEV__": true
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/agneym/react-avatar.git"
  },
  "bugs": {
    "url": "https://github.com/agneym/react-avatar/issues"
  }
}
